pipeline{
    agent any

    stages{
        stage('prep') {
            steps {
                script {
                    env.GIT_HASH = sh(
                        returnStdout: true, 
                        script: 'git rev-parse HEAD'
                        ).trim().take(7)
                }
            }
        }     
        stage("docker build"){
            steps{
                 echo "${env.BUILD_ID}-${env.GIT_HASH}"
                 }
        }
    }
}
