pipeline{
    agent any

    stages{
        stage('prep') {
            steps {
                script {
                    env.GIT_HASH = sh(
                        script: "git rev-parse HEAD').trim().take(7)",
                        returnStdout: true
                    ).trim()
                }
            }
        }     
        stage("docker build"){
            steps{
                 echo "${env.BUILD_ID}-${env.GIT_HASH}"
                 }
            }
        }
    }
}
